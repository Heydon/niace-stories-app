{
  "name": "niace-stories-app",
  "version": "0.0.1",
  "description": "The alpha application for NIACE, running on Meteor.",
  "main": "",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "genStoryFixtures": "mongo 127.0.0.1:3001/meteor --quiet --eval \"printjson(db.themes.find().toArray())\" | ./scripts/fillout.js 150 > app/private/data/stories.json",
    "stubTemplates": "sed \"s/<template name=\\\"/Template.stub('/g\" `find ./app/client -name *.html` |sed \"s/\\\">/');/g\" |grep Template.stub\\( > test/unit/stubs.js",
    "test": "npm run stubTemplates; rtd bgShell:killAll; rtd runOnce; TESTRESULT=$?; rtd bgShell:killAll; exit $TESTRESULT",
    "jshint": "jshint ./"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Heydon/niace-stories-app.git"
  },
